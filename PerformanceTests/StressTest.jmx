        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="Get authorization code" enabled="true">
          <stringProp name="WebDriverSampler.script">//WebDriver classes
var pkg = JavaImporter(org.openqa.selenium);
var support_ui = JavaImporter(org.openqa.selenium.support.ui.WebDriverWait);
var ui=JavaImporter(org.openqa.selenium.support.ui)
 
//captures sampler&apos;s start time
WDS.sampleResult.sampleStart();
WDS.sampleResult.getLatency();
WDS.log.info(&quot;Sample started&quot;);
 
//opens website
WDS.browser.get(&apos;https://tst-ids-01-svc.azurewebsites.net/connect/authorize?scope=openid+profile+email+offline_access+default.manage&amp;response_type=code&amp;redirect_uri=https://tst-ana-02-ana-svc.azurewebsites.net/api/analytics/swagger/oauth2-redirect.html&amp;client_id=tst-ana-02-ana&apos;);
 
//login
var userName = WDS.browser.findElement(pkg.By.id(&apos;Username&apos;));
userName.click();
userName.sendKeys([&apos;lisec@lisec.com&apos;]);

var continuebutton  = WDS.browser.findElement(pkg.By.name(&apos;button&apos;));
continuebutton.click();
 
var password = WDS.browser.findElement(pkg.By.id(&apos;Password&apos;)); 
password.click(); 
password.sendKeys([&apos;lisec&apos;]);

var loginbtnsignin = WDS.browser.findElement(pkg.By.name(&apos;button&apos;));
loginbtnsignin.click();
 
WDS.sampleResult.sampleEnd();</stringProp>
